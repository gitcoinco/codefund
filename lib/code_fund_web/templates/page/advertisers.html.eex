      </figure>
      <!-- End SVG Background Section -->
    </div>
    <!-- End Hero Section -->

    <!-- Front for -->
    <div class="container u-space-2-top u-space-3-bottom">
      <!-- Title -->
      <div class="w-md-80 w-lg-50 text-center mx-auto mb-9">
        <span class="u-label u-label--sm u-label--success mb-2">CodeFund</span>
        <h2 class="text-codefund-purple font-weight-normal">Who is <span class="font-weight-bold text-codefund-green">CodeFund</span> for?</h2>
        <p>SaaS companies. Training services. Developer announcements. More!</p>
      </div>
      <!-- End Title -->

      <div class="row align-items-lg-center">
        <div class="col-lg-5 order-lg-2 mb-7 mb-lg-0">
          <!-- Info -->
          <div class="pl-lg-7">
            <div class="mb-4">
              <h3 class="h5">SaaS Companies</h3>
              <p>If your product is typically sold to software developers, you are in the right place. We can get your products or services seen all over the world.</p>
            </div>

            <!-- Link -->
            <a class="media align-items-center shadow-sm p-4" href="#">
              <span class="d-flex mr-3">
                <img class="img-fluid u-avatar rounded-circle" src="<%= static_path(@conn, "/theme/img/icons/img2.png") %>" alt="Image Description">
              </span>
              <span class="media-body">
                <span class="d-flex justify-content-between align-items-center font-weight-bold">
                  Advertise with CodeFund <span class="fa fa-angle-right"></span>
                </span>
              </span>
            </a>
            <!-- End Link -->
          </div>
          <!-- End Info -->
        </div>

        <div class="col-lg-7 order-lg-1 mb-7 mb-lg-0">
          <!-- SVG Icon -->
          <img src="<%= static_path(@conn, "/theme/svg/flat-icons/sitting-woman-flat-concept-illustration.svg") %>" alt="SVG Illustration">
          <!-- End SVG Icon -->
        </div>
      </div>
    </div>
    <!-- End Front for -->

