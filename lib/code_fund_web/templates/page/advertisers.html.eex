  <!-- ========== MAIN ========== -->
  <main id="content" role="main">
    <!-- Hero Section -->
    <div class="position-relative u-gradient-half-primary-v3">
      <div class="container u-space-4-top u-space-5-top--md u-space-4-top--lg u-space-2-bottom">
        <div class="row align-items-lg-center">
          <div class="col-lg-5 mb-7 mb-lg-0">
            <!-- Info -->
            <h1 class="mb-4 text-codefund-purple">Your ads will reach the <span class="text-codefund-green font-weight-bold">right</span> audience.</h1>
            <p>CodeFund provides a turn-key advertising solution to place your ad in front of software developers of many programming languages.</p>
            <!-- End Info -->
          </div>

          <div class="col-lg-7">
            <!-- SVG Icon -->
            <img src="<%= static_path(@conn, "/theme/svg/flat-icons/we-have-an-idea-flat-concept-illustration.svg") %>">
            <!-- End SVG Icon -->
          </div>
        </div>
      </div>

      <!-- SVG Background -->
      <figure class="position-absolute-bottom-0">
