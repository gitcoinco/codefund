    <div class="container">
      <!-- Logo container-->
      <div>
        <a href="/" class="logo">
          <img src="<%= static_path(@conn, "/images/codefund-logo.png") %>" style="height: 25px;">
        </a>
      </div>
      <!-- End Logo container-->
      
      <div class="menu-extras">
        <div class="menu-item">
          <!-- Mobile menu toggle-->
          <a class="navbar-toggle">
            <div class="lines">
              <span></span>
              <span></span>
              <span></span>
            </div>
          </a>
          <!-- End mobile menu toggle-->
        </div>
      </div>

      <div id="navigation">
        <!-- Navigation Menu-->
        <ul class="navigation-menu">
          <li class="has-submenu ">
            <a href="#home">Home</a>
          </li>

          <li class="has-submenu ">
            <a href="#features">Why CodeFund?</a>
          </li>

          <li class="has-submenu">
            <a href="#ethical_advertising">Ethical Advertising</a>
          </li>

          <li class="has-submenu">
