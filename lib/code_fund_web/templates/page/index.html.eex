  <link rel="shortcut icon" href="<%= static_path(@conn, "/images/codefund-logo-square.png") %>">
  <title>CodeFund</title>

  <!-- Browser Icons -->
  <link rel="shortcut icon" href="<%= static_path(@conn, "/images/codefund-logo-square.png") %>">
  <link rel="apple-touch-icon" sizes="180x180" href="<%= static_path(@conn, "/apple-touch-icon.png") %>">
  <link rel="icon" type="image/png" sizes="32x32" href="<%= static_path(@conn, "/favicon-32x32.png") %>">
  <link rel="icon" type="image/png" sizes="16x16" href="<%= static_path(@conn, "/favicon-16x16.png") %>">
  <link rel="manifest" href="<%= static_path(@conn, "/site.webmanifest") %>">
  <link rel="mask-icon" href="<%= static_path(@conn, "/safari-pinned-tab.svg") %>" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">

  <!-- Google Fonts -->
  <link href="//fonts.googleapis.com/css?family=Nunito:400,600,700" rel="stylesheet">

  <!-- Magnific-popup -->
  <link rel="stylesheet" href="<%= static_path(@conn, "/canvab/css/magnific-popup.css") %>">

  <!-- Bootstrap core CSS -->
  <link href="<%= static_path(@conn, "/canvab/css/bootstrap.min.css") %>" rel="stylesheet">

  <!-- materialdesign icons CSS -->
  <link href="<%= static_path(@conn, "/canvab/css/materialdesignicons.min.css") %>" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="<%= static_path(@conn, "/canvab/css/menu.css") %>" rel="stylesheet">
  <link href="<%= static_path(@conn, "/canvab/css/style.css") %>" rel="stylesheet">

  <style>
    .navbar-brand img {
      height: 30px;
      top: 25px;
    }
    .navbar.shrink .navbar-brand img {
      height: 20px;
      top: 20px;
    }
    #macbook-slideshow {
      position: relative;
    }
    #macbook-slideshow img.macbook {
      z-index: 1;
      position: absolute;
    }
    #macbook-slideshow > img.slide {
      z-index: 0;
    }
    #macbook-slideshow > img.slide ~ img.slide {
      display: none;
    }
  </style>
</head>

<body id="page-top">

  <!-- TAGLINE HEADER START-->
